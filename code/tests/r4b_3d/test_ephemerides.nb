Cell[BoxData[
 RowBox[{
  RowBox[{"getEphemeridesOnDay", "[", "day_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"Get", " ", "full", " ", "ephemerides", " ", "for", " ", "Sun"}], 
    ",", 
    RowBox[{"Earth", " ", "and", " ", "Mars", " ", "on", " ", "day", " ", 
     RowBox[{"(", 
      RowBox[{"angles", " ", "in", " ", "degrees"}], ")"}]}], ",", 
    RowBox[{"except", " ", "date"}]}], "*)"}], "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "max", ",", "dayLower", ",", "dayUpper", ",", "dayIncrement", ",", "sun",
       ",", "earth", ",", "mars"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ephLength", "=", 
      RowBox[{"ephemeridesEarth", "//", "Length"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"day", "+", "3"}], ">", "ephLength"}], "||", 
        RowBox[{"day", "<", 
         RowBox[{"-", "1"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
       "\"\<Day out of range. Read-in ephemerides must be day must be in \
interval [-1,\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"ephLength", "-", "3"}], "]"}], "<>", "\"\<]\>\""}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"dayLower", "=", 
         RowBox[{"Floor", "[", "day", "]"}]}], " ", ";", 
        "\[IndentingNewLine]", 
        RowBox[{"dayUpper", "=", 
         RowBox[{"Ceiling", "[", "day", "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"dayIncrement", "=", 
         RowBox[{"day", "-", "dayLower"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"earth", ",", "mars"}], "}"}], "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"ephLower", "=", 
             RowBox[{
              RowBox[{"ToExpression", "[", 
               RowBox[{"\"\<ephemerides\>\"", "<>", "body"}], "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"dayLower", "+", "3"}], "]"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"ephLower", "=", 
             RowBox[{"Drop", "[", 
              RowBox[{"ephLower", ",", 
               RowBox[{"{", "3", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"ephUpper", "=", 
             RowBox[{
              RowBox[{"ToExpression", "[", 
               RowBox[{"\"\<ephemerides\>\"", "<>", "body"}], "]"}], "[", 
              RowBox[{"[", 
               RowBox[{"dayUpper", "+", "3"}], "]"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"ephUpper", "=", 
             RowBox[{"Drop", "[", 
              RowBox[{"ephUpper", ",", 
               RowBox[{"{", "3", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"diff", "=", 
             RowBox[{"ephUpper", "-", "ephLower"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"ephIncrement", "=", 
             RowBox[{"diff", "*", "dayIncrement"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"dayEph", "=", 
             RowBox[{"ephLower", "+", "ephIncrement"}]}]}], ",", 
           RowBox[{"{", 
            RowBox[{"body", ",", 
             RowBox[{"{", 
              RowBox[{"\"\<Earth\>\"", ",", "\"\<Mars\>\""}], "}"}]}], 
            "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"sun", "=", "earth"}], ";", "\[IndentingNewLine]", 
        RowBox[{
