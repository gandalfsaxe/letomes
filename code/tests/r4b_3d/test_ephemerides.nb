Cell[BoxData[
 RowBox[{
  RowBox[{"getCoordinatesOnDayRad", "[", "day_", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ephOnDay", ",", "coords"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ephOnDay", "=", 
      RowBox[{"getEphemeridesOnDay", "[", "day", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coords", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"\"\<\>\"", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Length", "@", "ephOnDay"}], ",", 
          RowBox[{"Length", "@", "ephOnDay"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", 
        RowBox[{"Length", "@", "ephOnDay"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", " ", 
       RowBox[{
        RowBox[{"coords", "[", 
         RowBox[{"[", "i", "]"}], "]"}], "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"ephOnDay", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "2"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"ephOnDay", "[", 
            RowBox[{"[", "2", "]"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "2"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"ephOnDay", "[", 
            RowBox[{"[", "3", "]"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"i", "+", "2"}], "]"}], "]"}]}], "}"}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"coords", "[", 
       RowBox[{"[", "2", "]"}], "]"}], "=", 
      RowBox[{
       RowBox[{"N", "@", 
